<configuration status="error">
  <appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5r %5p [%t] (%C{1}:%L) - %m%n"/>
    </Console>
  </appenders>
  <loggers>
    <logger name="org.apache.zookeeper" level="info">
    </logger>
  	<root level="error">
      <appender-ref ref="STDOUT"/>
    </root>
  </loggers>
</configuration>
