<?xml version="1.0"?>
<configuration status="error" dest="err">
  <appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5r %5p [%t] (%C{1}:%L) - %m%n"/>
    </Console>
  </appenders>
  <loggers>
    <logger name="edu.uw.zookeeper" level="trace">
      <filters>
        <MarkerFilter marker="EDU_UW_ZOOKEEPER_PROTOCOL_PING" onMatch="DENY" onMismatch="NEUTRAL"/>
        <MarkerFilter marker="EDU_UW_ZOOKEEPER_PROTOCOL_NET" onMatch="DENY" onMismatch="NEUTRAL"/>
        <MarkerFilter marker="EDU_UW_ZOOKEEPER_PROTOCOL_NETTY" onMatch="DENY" onMismatch="NEUTRAL"/>
      </filters>
    </logger>
    <logger name="io.netty" level="warn">
    </logger>
  	<root level="error">
      <appender-ref ref="STDOUT"/>
    </root>
  </loggers>
</configuration>
